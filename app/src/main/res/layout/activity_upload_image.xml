<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GeneralModule.UploadImage">


    <ImageView
        android:id="@+id/image"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:onClick="chooseImage"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_baseline_anchor_24" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Move Next"
        android:id="@+id/moveNext"
        android:layout_below="@+id/image"
        android:onClick="Register2PageGo"/>

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="363dp"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24"
        android:onClick="backButton5"/>


</RelativeLayout>